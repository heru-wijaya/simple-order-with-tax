{
  "name": "order-project",
  "version": "1.0.0",
  "description": "order project with tax",
  "author": "Heru Ganda Wijaya <heru.ganda.wijaya@gmail.com>",
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "unit-test": "NODE_ENV=test nyc --reporter=text ava --verbose",
    "lint": "eslint --fix --ext .js,.vue ."
  },
  "keywords": [
    "nodejs",
    "sequelize",
    "express",
    "ava",
    "docker"
  ],
  "dependencies": {
    "ava": "^1.2.1",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "joi": "^14.3.1",
    "lodash": "^4.17.11",
    "mysql2": "^1.6.5",
    "nyc": "^13.3.0",
    "sequelize": "^4.42.1",
    "sinon": "^7.2.5"
  },
  "nyc": {
    "per-file": true,
    "lines": 80,
    "statements": 80,
    "functions": 80,
    "branches": 80,
    "reporter": [
      "lcov"
    ],
    "exclude": [
      "**/modules/src/repositories/*.js",
      "**/modules/tests/**"
    ]
  },
  "ava": {
    "files": [
      "**/modules/tests/unit/**/*.test.js"
    ],
    "failFast": false,
    "failWithoutAssertions": false,
    "tap": false,
    "powerAssert": true
  },
  "devDependencies": {
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0"
  }
}
